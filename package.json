{
  "name": "createrest-lerna",
  "private": true,
  "scripts": {
    "build": "lerna run build --stream",
    "clean": "lerna run clean --stream",
    "coverage": "lerna run coverage --stream",
    "docs": "lerna run docs --scope createrest",
    "metapak": "metapak || exit 0",
    "postinstall": "lerna bootstrap; npm run metapak --silent",
    "postpublish": "git push --all origin && git push --tags origin && lerna run docs:publish",
    "prepublish": "lerna run build --stream && lerna run test --stream",
    "publish": "lerna publish",
    "report": "lerna run report --stream",
    "test": "lerna run build && lerna run test"
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^1.1.0",
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-node8": "^1.2.0",
    "babel-register": "^6.26.0",
    "buildbranch": "^2.0.0",
    "coveralls": "^2.13.1",
    "esdoc": "^1.0.3",
    "eslint": "^4.8.0",
    "eslint-config-atomix-base": "^4.3.2",
    "eslint-plugin-import": "^2.2.0",
    "lerna": "^2.4.0",
    "metapak": "0.0.21",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {}
}
